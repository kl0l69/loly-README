# Python
print("أنا Arsinek")  # Python

# C
# gcc example.c -o example && ./example
#include <stdio.h>
int main() { printf("أنا Arsinek\n"); return 0; }  # C

# C++
#include <iostream>
using namespace std;
int main() { cout << "أنا Arsinek" << endl; return 0; }  # C++

# Java
public class Main { public static void main(String[] args) { System.out.println("أنا Arsinek"); } }  # Java

# JavaScript (Node.js / Browser)
console.log("أنا Arsinek");  // JavaScript

# HTML + JS
# <script>document.write("أنا Arsinek"); console.log("أنا Arsinek");</script>  # HTML/JS

# PHP
<?php echo "أنا Arsinek"; ?>  # PHP

# Ruby
puts "أنا Arsinek"  # Ruby

# Go
package main; import "fmt"; func main() { fmt.Println("أنا Arsinek") }  # Go

# C#
using System; class Program { static void Main() { Console.WriteLine("أنا Arsinek"); } }  # C#

# Swift
print("أنا Arsinek")  # Swift

# Kotlin
fun main() { println("أنا Arsinek") }  # Kotlin
